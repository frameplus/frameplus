{
  "name": "webapp",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "wrangler pages dev dist",
    "deploy": "npm run build && wrangler pages deploy dist",
    "dev:sandbox": "wrangler pages dev dist --d1=frameplus-erp --local --ip 0.0.0.0 --port 3000",
    "db:migrate:local": "wrangler d1 migrations apply frameplus-erp --local",
    "db:seed": "wrangler d1 execute frameplus-erp --local --file=./seed.sql"
  },
  "dependencies": {
    "hono": "^4.11.9"
  },
  "devDependencies": {
    "@hono/vite-build": "^1.2.0",
    "@hono/vite-dev-server": "^0.18.2",
    "acorn": "^8.15.0",
    "vite": "^6.3.5",
    "wrangler": "^4.4.0"
  }
}
